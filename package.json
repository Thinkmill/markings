{
  "name": "@markings/repo",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/Thinkmill/markings",
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@changesets/changelog-github": "^0.2.1",
    "@changesets/cli": "^2.5.2",
    "@manypkg/cli": "^0.11.0",
    "@preconstruct/cli": "^1.1.8",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "prettier": {},
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "release": "yarn preconstruct build && yarn changeset publish",
    "test-gatsby": "cd test-gatsby && gatsby develop",
    "test": "jest"
  }
}
